{
  "name": "matlab",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "expression"
      }
    },
    "operation": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "factor"
        },
        {
          "type": "SYMBOL",
          "name": "_operator"
        },
        {
          "type": "SYMBOL",
          "name": "factor"
        }
      ]
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "_eq"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "operation"
            },
            {
              "type": "SYMBOL",
              "name": "factor"
            },
            {
              "type": "SYMBOL",
              "name": "vector_definition"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_semi_colon"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]"
    },
    "factor": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_number"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "_semi_colon": {
      "type": "STRING",
      "value": ";"
    },
    "_eq": {
      "type": "STRING",
      "value": "="
    },
    "_operator": {
      "type": "PATTERN",
      "value": "[+\\-*/%]"
    },
    "_number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "vector_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "factor"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

